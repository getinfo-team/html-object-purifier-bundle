{
    "name": "getinfo-team/html-object-purifier-bundle",
    "description": "Symfony bundle for using HTMLPurifier integration for php objects",
    "keywords": ["htmlpurifier", "html", "purifier", "symfony", "object", "objects", "model", "xss", "annotation", "annotations"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kenny1911",
            "email": "o-muzyka@mail.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "^1.0",
        "exercise/htmlpurifier-bundle": "^3.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/property-access": "^4.0|^5.0",
        "symfony/property-info": "^4.0|^5.0"
    },
    "require-dev": {
        "phpdocumentor/reflection-docblock": "^3.0|^4.0|^5.0",
        "symfony/phpunit-bridge": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "GetInfoTeam\\HTMLObjectPurifierBundle\\": "./"
        },
        "exclude-from-classmap": [
            "./Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "GetInfoTeam\\HTMLObjectPurifierBundle\\Tests\\": "./Tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "phpunit": "simple-phpunit"
    }
}
